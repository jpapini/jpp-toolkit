{
    "name": "@jpp-toolkit/utils",
    "version": "0.0.14",
    "description": "Useful utilities and helpers for JS/TS projects.",
    "keywords": [
        "jpp",
        "utils"
    ],
    "homepage": "https://github.com/jpapini/jpp-toolkit/tree/main/packages/utils#readme",
    "bugs": {
        "url": "https://github.com/jpapini/jpp-toolkit/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jpapini/jpp-toolkit.git",
        "directory": "packages/utils"
    },
    "license": "MIT",
    "author": "Julien Papini <julien.papini@gmail.com> (https://github.com/jpapini)",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.mts",
            "default": "./dist/index.mjs"
        },
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "src"
    ],
    "scripts": {
        "typecheck": "tsc --noEmit --pretty",
        "dev": "build-lib --watch",
        "build": "build-lib"
    },
    "packageManager": "pnpm@10.25.0",
    "engines": {
        "node": "24",
        "pnpm": "10"
    },
    "volta": {
        "extends": "../../package.json"
    },
    "publishConfig": {
        "access": "public"
    }
}
